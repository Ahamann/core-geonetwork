                                    codeListValue="revision"/>
                            </gmd:dateType>
                        </gmd:CI_Date>
                    </gmd:date>
                </template>
            </field>
            <fieldset xpath="quality/spec"> </fieldset>-->
        </section>
        <section name="metadata">
            <field xpath="/gmd:MD_Metadata/gmd:language"/>
            <field xpath="/gmd:MD_Metadata/gmd:dateStamp"/>
        </section>
    </tab>
</config>